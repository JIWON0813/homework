MYSQL에 순차적으로 등록

CREATE DATABASE HOMEWORK;

CREATE TABLE `REVIEW` (
	`REVIEW_ID` VARCHAR(255) PRIMARY KEY,
	`CONTENT` VARCHAR(4000) NULL,
	`PHOTO_ID_LIST` VARCHAR(4000) NULL,
	`USER_ID` VARCHAR(255) NOT NULL,
	`PLACE_ID` VARCHAR(255) NOT NULL,
	`WRITE_DT` DATETIME NOT NULL DEFAULT NOW(),
	`MODIFY_DT` DATETIME NOT NULL DEFAULT NOW() ON UPDATE NOW(),
	INDEX `index1` (`REVIEW_ID`, `USER_ID`, `PLACE_ID`)
);


CREATE TABLE `review_history` (
	`SEQ` INT(11) NOT NULL AUTO_INCREMENT,
	`REVIEW_ID` VARCHAR(255) NOT NULL,
	`USER_ID` VARCHAR(255) NOT NULL,
	`STATUS` CHAR(1) NOT NULL,
	`BEFORE_PHOTO` VARCHAR(4000),
	`AFTER_PHOTO` VARCHAR(4000),
	`BEFORE_CONTENT` VARCHAR(4000),
	`AFTER_CONTENT` VARCHAR(4000),
	`WRITE_DT` DATETIME,
	PRIMARY KEY (`SEQ`) USING BTREE,
);


CREATE TABLE `USER` (
	`USER_ID` VARCHAR(255) NOT NULL,
	`POINT` INT(10) UNSIGNED ZEROFILL,
	`WRITE_DT` DATETIME,
	`MODIFY_DT` DATETIME,
	PRIMARY KEY (`USER_ID`) USING BTREE
);


CREATE TABLE POINT_HISTORY(
	`SEQ` INT(11) NOT NULL AUTO_INCREMENT,
	`STATUS` CHAR(1) NOT NULL,
	`USER_ID` VARCHAR(255) NOT NULL,
	`BRFORE_POINT` INT(10),
	`AFTER_POINT` INT(10),
	`WRITE_DT` DATETIME NOT NULL,
	PRIMARY KEY (`SEQ`) USING BTREE,
	INDEX `PLACE_ID` (`USER_ID`) USING BTREE
);

CREATE TABLE `PLACE` (
	`PLACE_ID` VARCHAR(255) NOT NULL,
	`USER_ID` VARCHAR(255) NOT NULL,
	`WRITE_DT` DATETIME NOT NULL,
	PRIMARY KEY (`PLACE_ID`) USING BTREE,
	INDEX `PLACE_ID` (`PLACE_ID`, `USER_ID`) USING BTREE
);


CREATE TABLE `PLACE_HISTORY` (
	`SEQ` INT(11) NOT NULL AUTO_INCREMENT,
	`STATUS` CHAR(1) NOT NULL,	
	`PLACE_ID` VARCHAR(255) NOT NULL,
	`USER_ID` VARCHAR(255) NOT NULL,
	`WRITE_DT` DATETIME NOT NULL,
	PRIMARY KEY (`SEQ`) USING BTREE,
	INDEX `PLACE_ID` (`PLACE_ID`) USING BTREE
);
